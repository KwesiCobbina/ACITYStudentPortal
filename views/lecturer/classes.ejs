      <!-- Wrap the rest of the page in another container to center all the content. -->

      <div class="container marketing">
          <div class="row mt-5">
              <div class="container-fluid mt-5" style="display: flex">
                <table class="table table-striped table-dark">
                    <thead>
                        <tr>
                        <th>#</td>
                        <th>Program</th>
                        <th>Course</th>
                        <th>Year</th>
                        <th>Class</th>
                        <th>Action1</th>
                        <th>Action2</th>
                        </tr>
                    </thead>
                    <tbody>
                        <% data.forEach((i,e)=>{ %>
                        <tr>
                                <td><%=e+1%></td>
                                <td><%=i.programID.name%></td>
                                <td><%=i.course.name%></td>
                                <td><%=i.year%></td>
                                <td><%=i.programID.code + ""+ i.year%></td>
                                <td><a href="/lecturer/grader?program=<%=i.programID.code%>&course=<%=i.course.code%>"><button class="btn btn-primary">Grade class</button></a></td>
                                <td><a href="/lecturer/viewer?program=<%=i.programID.code%>&course=<%=i.course.code%>"><button class="btn btn-primary">View class</button></a></td>
                        </tr>
                        <% }) %>
                    </tbody>
                    
                </table>
                  
                
              </div>
          </div>

